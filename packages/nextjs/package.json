{
  "name": "@t3-oss/env-nextjs",
  "version": "0.11.1",
  "type": "module",
  "keywords": ["create-t3-app", "environment variables", "zod", "nextjs"],
  "author": "Julius Marminge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t3-oss/t3-env",
    "directory": "packages/nextjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./presets-zod": {
      "types": "./dist/presets-zod.d.ts",
      "default": "./dist/presets-zod.js"
    }
  },
  "files": ["dist", "package.json", "LICENSE", "README.md"],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w --no-clean",
    "typecheck": "tsc --noEmit",
    "prepack": "bun ../../replace-workspace-protocol.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "zod": "^3.24.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "bunchee": "^6.3.2",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  }
}
