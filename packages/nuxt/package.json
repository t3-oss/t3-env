{
  "name": "@t3-oss/env-nuxt",
  "version": "0.12.0",
  "type": "module",
  "keywords": [
    "create-t3-app",
    "environment variables",
    "zod",
    "nuxt",
    "vue"
  ],
  "author": "Julius Marminge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t3-oss/t3-env",
    "directory": "packages/nuxt"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./presets-zod": {
      "types": "./dist/presets-zod.d.ts",
      "default": "./dist/presets-zod.js"
    },
    "./presets-valibot": {
      "types": "./dist/presets-valibot.d.ts",
      "default": "./dist/presets-valibot.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --no-clean",
    "typecheck": "tsc --noEmit",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "zod": "^3.24.0 || ^4.0.0-beta.0",
    "valibot": "^1.0.0-beta.7 || ^1.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "zod": {
      "optional": true
    },
    "valibot": {
      "optional": true
    }
  },
  "devDependencies": {
    "bunchee": "6.5.1",
    "typescript": "5.8.3",
    "zod": "3.24.3"
  }
}
